{
  "name": "onemindai",
  "version": "10.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:all": "concurrently \"npm run dev\" \"npm run server\" \"npm run proxy\"",
    "dev:safe": "node server/server-monitor.cjs",
    "dev:guardian": "concurrently \"npm run dev:safe\" \"npm run guardian\"",
    "server": "node server/balance-api.cjs",
    "proxy": "node server/ai-proxy.cjs",
    "proxy:dev": "node --watch server/ai-proxy.cjs",
    "proxy:test": "node server/test-proxy.cjs",
    "guardian": "node server/code-guardian/index.cjs",
    "guardian:ui": "cd code-guardian && npm run dev",
    "guardian:install": "cd code-guardian && npm install",
    "guardian:all": "concurrently \"npm run guardian\" \"npm run guardian:ui\"",
    "build": "vite build",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:full": "playwright test tests/e2e/comprehensive-e2e.spec.ts",
    "test:e2e:full:headed": "playwright test tests/e2e/comprehensive-e2e.spec.ts --headed",
    "start": "serve -s dist -l 3000",
    "preview": "vite preview",
    "chaos": "cd chaos-monkey && npm start",
    "chaos:install": "cd chaos-monkey && npm install",
    "chaos:all": "cd chaos-monkey && node cli.js --all",
    "chaos:api": "cd chaos-monkey && node cli.js --suite api",
    "chaos:security": "cd chaos-monkey && node cli.js --suite security",
    "chaos:load": "cd chaos-monkey && node cli.js --suite load",
    "chaos:report": "cd chaos-monkey && node generate-report.js",
    "chaos:analysis": "cd chaos-monkey/analysis && npm start",
    "chaos:analysis:install": "cd chaos-monkey/analysis && npm install"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@google/generative-ai": "^0.24.1",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-slot": "^1.2.4",
    "@supabase/auth-helpers-react": "^0.15.0",
    "@supabase/supabase-js": "^2.86.0",
    "@types/pdfmake": "^0.2.12",
    "chart.js": "^4.5.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "docx": "^9.5.1",
    "dompurify": "^3.0.6",
    "echarts": "^5.4.3",
    "framer-motion": "^12.23.26",
    "html-docx-js": "^0.3.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.4",
    "lucide-react": "^0.553.0",
    "mammoth": "^1.11.0",
    "marked": "^17.0.1",
    "mermaid": "^11.4.0",
    "openai": "^6.8.1",
    "pdfmake": "^0.2.20",
    "plotly.js-dist-min": "^2.35.2",
    "react": "18.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "18.2.0",
    "react-plotly.js": "^2.6.0",
    "recharts": "^2.10.3",
    "tailwind-merge": "^3.4.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^24.10.1",
    "@types/react": "18.2.66",
    "@types/react-dom": "18.2.22",
    "@types/react-plotly.js": "^2.6.3",
    "@vitejs/plugin-react": "4.2.1",
    "autoprefixer": "10.4.19",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "glob": "^10.3.10",
    "helmet": "^7.2.0",
    "node-fetch": "^2.7.0",
    "postcss": "8.4.38",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.5",
    "vite": "5.2.0",
    "vitest": "^4.0.15",
    "ws": "^8.16.0"
  }
}
